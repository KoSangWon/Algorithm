    "import sys\n",
    "sys.setrecursionlimit(2000)\n",
    "\n",
    "n = int(input())\n",
    "dp = [0] * 1001\n",
    "\n",
    "dp[1] = 1\n",
    "dp[2] = 3\n",
    "for i in range(3, n + 1):\n",
    "    dp[i] = (dp[i - 1] + 2 * dp[i - 2]) % 10007\n",
    "\n",
    "print(dp[n])\n",
    "\n"
   ]
