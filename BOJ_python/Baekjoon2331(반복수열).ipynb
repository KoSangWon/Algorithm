   "source": [
    "A, P = map(int, input().split())\n",
    "visited = [0] * 300001\n",
    "next = 0\n",
    "\n",
    "while visited[A] != 3:\n",
    "    visited[A] += 1\n",
    "    next = 0\n",
    "    for i in str(A):\n",
    "        next += pow(int(i), P)\n",
    "    A = next\n",
    "    \n",
    "count = 0\n",
    "for i in range(300001):\n",
    "    if visited[i] == 1:\n",
    "        count += 1\n",
    "\n",
    "print(count)"
   ]
